{
  "name": "Graph dependency",
  "version": "1.2",
  "description": "Create a simple graph dep on trello",
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "permissions": ["storage", "activeTab"],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.trello.com/*"],
      "js": [
        "graphdep.js"
      ]
    }
  ]
}
