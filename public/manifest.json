{
  "name": "Graph dependency",
  "version": "1.2",
  "description": "Create a simple graph dep on trello",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo-16.png",
      "32": "logo-32.png",
      "48": "logo-48.png",
      "128": "logo-128.png"
    }
  },
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "contextMenus",
    "activeTab",
    "management"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*.trello.com/*"],
      "js": [
        "librairies/jquery-3.4.1.min.js",
        "librairies/cytoscape-3.14.0.min.js",
        "graphdep.js"
      ]
    }
  ]
}
